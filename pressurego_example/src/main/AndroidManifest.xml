<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.algorigo.pressurego_example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PressureGoApp">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RxActivity"/>
        <activity android:name=".BasicActivity"/>
        <activity android:name=".BasicPDMSDeviceActivity"/>
        <activity android:name=".NotificationActivity"/>
        <activity android:name=".PDMSDeviceActivity"/>
        <activity android:name=".RxPDMSDeviceActivity"/>
    </application>

</manifest>